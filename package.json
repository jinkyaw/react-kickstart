{
  "name": "react-kickstart",
  "version": "6.0.0",
  "description": "just another react + webpack boilerplate.",
  "private": true,
  "author": "Alessandro Arnodo <alessandro@arnodo.net> (https://alessandro.arnodo.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vesparny/react-kickstart/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vesparny/react-kickstart.git"
  },
  "homepage": "https://github.com/vesparny/react-kickstart",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --coverage",
    "dev": "node devServer",
    "start": "cross-env NODE_ENV=production node prodServer",
    "prebuild": "npm run build:clean",
    "build:clean": "rimraf build",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --color -p --progress",
    "gh-pages": "gh-pages --dist build",
    "precommit": "lint-staged",
    "lint": "standard",
    "format": "prettier --write --single-quote 'src/**/*.js' && standard --fix",
    "flow": "flow"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "jest",
      "expect",
      "it",
      "test",
      "describe"
    ]
  },
  "lint-staged": {
    "linters": {
      "src/*/**/*.js": [
        "npm run format",
        "git add"
      ]
    }
  },
  "dependencies": {
    "glamor": "^2.20.24",
    "glamorous": "^3.1.0",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "prop-types": "^15.5.6",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-router-dom": "^4.0.0",
    "reflexbox": "^2.2.3",
    "sanitize.css": "^5.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.43.1",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.28.0",
    "jest-cli": "^19.0.2",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "prettier": "^0.22.0",
    "react-addons-test-utils": "^15.5.1",
    "redbox-react": "^1.3.6",
    "rimraf": "^2.6.1",
    "standard": "^10.0.1",
    "standard-version": "^4.0.0",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
